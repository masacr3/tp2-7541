package javaapplication1;

import java.util.Scanner;

/**
 *
 * @author @masacr3
 */
public class JavaApplication1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner teclado = new Scanner(System.in);

        int a,b ;

        System.out.println("ingrese 2 numeros: ");
        System.out.print("numero1 : ");
        a = teclado.nextInt();
        System.out.print("numero2 : ");
        b = teclado.nextInt();

        if ( esta_en_rango(a,b) ){
            if ( a == b){
                System.out.println("son iguales");
            }
            else{
                if (a > b) {
                    System.out.println("El mayor es numero 1: "+a);
                }
                else{
                    System.out.println("El mayor es numero 2: "+b);
                }
            }
        }
    }

    //Cuando decian entre 0 y 20 no se especifico mas si incluian
    //al 0 y 20 o lo excluian
    //para mi implementacion inclui las al 0 y 20
    public static boolean esta_en_rango(int a, int b){
        boolean ok = false;

        if (a >= 0 && b >= 0 && a <= 20 && b <= 20){
            ok = true;
        }

        return ok;
    }

}
